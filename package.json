{
  "name": "foknofolies-4",
  "version": "1.0.0",
  "description": "Site web de la quatrième édition des FoknoFolies.",
  "main": "gulpfile.js",
  "author": "Arthur Fauquenot <arthurfauquenot@yahoo.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurfauq/foknofolies-2.git"
  },
  "homepage": "https://github.com/arthurfauq/foknofolies-2#readme",
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "serve:dist": "gulp serve",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint . --ignore-path .gitignore && stylelint \"src/**/*.scss\" && prettier --ignore-path .gitignore --check \"**/*.{html,css,scss,js,json,md}\"",
    "lint:fix": "eslint . --ignore-path .gitignore --fix && stylelint \"src/**/*.scss\" --fix && prettier --ignore-path .gitignore --check \"**/*.{html,css,scss,js,json,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{html,css,scss,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "font-awesome": "^4.7.0",
    "fullpage.js": "2.9.7",
    "jquery": "^3.3.1",
    "jquery.waitforimages": "^2.4.0",
    "moment": "^2.24.0",
    "owl.carousel": "^2.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/googlemaps": "^3.30.19",
    "browser-sync": "^2.18.8",
    "del": "^3.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^2.1.2",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.8",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.15.2",
    "run-sequence": "^1.2.2",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "license": "MIT"
}
